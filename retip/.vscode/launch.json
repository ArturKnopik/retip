{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "debug-en-dark",
            "request": "launch",
            "type": "dart",
            "toolArgs": [
                "--dart-define",
                "LOCALE=EN",
                "--dart-define",
                "MODE=DARK"
            ],
        },
        {
            "name": "debug-pl-light",
            "request": "launch",
            "type": "dart",
            "toolArgs": [
                "--dart-define",
                "LOCALE=PL",
                "--dart-define",
                "MODE=LIGHT"
            ],
        },
        {
            "name": "profile",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "release",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        },
    ]
}